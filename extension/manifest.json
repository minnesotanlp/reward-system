{
  "name": "REWARD",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "activeTab",
    "background",
    "declarativeContent",
    "notifications"
  ],
  "host_permissions": [
    "http://127.0.0.1:5000/*",
    "https://www.overleaf.com/",
    "*://*.overleaf.com/*"
  ],
  "action": {
    "default_title": "ReWARD",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.overleaf.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
